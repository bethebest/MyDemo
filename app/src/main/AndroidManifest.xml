<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="github.cccxm.mydemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".android.view.circle.CircleListActivity" />
        <activity android:name=".android.view.circle.CardViewCircleActivity" />
        <activity android:name=".android.effect.animation.AnimationListActivity" />
        <activity android:name=".android.material.bar.AppBarListActivity" />
        <activity android:name=".android.material.bar.AppBarSimpleActivity" />
        <activity android:name=".android.effect.linkage.LinkageListActivity" />
        <activity
            android:name=".android.effect.linkage.ListPushBarActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".android.effect.linkage.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".android.effect.linkage.ListHideBarActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.LearnDetailActivity"></activity>
    </application>

</manifest>